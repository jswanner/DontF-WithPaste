{
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "defaults.js",
        "content.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "description": "Prevents the blocking of copying from & pasting into input fields",
  "homepage_url": "https://github.com/jswanner/DontFuckWithPaste",
  "manifest_version": 2,
  "name": "Don't Fuck With Paste",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "storage"
  ],
  "version": "1.1"
}
